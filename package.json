{
  "name": "career-advisor-backend",
  "version": "1.0.0",
  "description": "Backend API for Career Advisor application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "load-test": "artillery run artillery-config.yml",
    "load-test:quick": "artillery run artillery-quick-test.yml",
    "load-test:stress": "artillery run artillery-stress-test.yml",
    "load-test:report": "artillery run artillery-config.yml --output report.json && artillery report report.json --output report.html"
  },
  "keywords": [
    "career",
    "jobs",
    "api",
    "remote",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/dialogflow-cx": "^5.3.0",
    "@google-cloud/documentai": "^9.5.0",
    "@google-cloud/speech": "^7.2.1",
    "@google-cloud/storage": "^7.17.2",
    "@google-cloud/vertexai": "^1.10.0",
    "@google-cloud/video-intelligence": "^6.2.1",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "firebase-admin": "^12.0.0",
    "googleapis": "^164.0.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.0",
